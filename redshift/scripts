   id       bigint          DEFAULT "identity"(306025, 0, '1,1'::text),
   game_id  integer,
   c_time   timestamp,
   uid      varchar(50),
   appid    varchar(50),
   appuid   varchar(50),
   ip       varchar(500)
);

