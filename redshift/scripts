####### test sample db
DROP TABLE public.user;
CREATE TABLE public.user
(
   name varchar(50),
   password varchar(50)
);

select * from public.user;

copy public.user from 's3://leo.emr/logs/test.log' 
credentials 'aws_access_key_id=xxxx;aws_secret_access_key=xxxx' 
delimiter ',';

select * from public.user;

COMMIT;

